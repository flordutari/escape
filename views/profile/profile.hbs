<div class="events-list">
  {{> navbar}}
  <div id="profile" class="events">
    <a href="/">homepage</a>
    <h1>{{user.username}}'s profile</h1>
    <img src="{{user.imageUrl}}" alt="user image">
    <p>{{user.description}}</p>
    {{#if isOwnProfile}}
    <a href="/profile/edit">Edit profile</a>
    {{/if}}
    {{#each user.myEvents}}
    <img src="{{this.escapeRoom.image}}">
    {{/each}}
  </div>
</div>
