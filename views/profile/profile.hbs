<div class="events-list">
  {{> navbar}}
  <div id="profile" class="events">
    <h1>{{user.username}}'s profile</h1>
    <img id="profile-img" src="{{user.imageUrl}}" alt="user image">
    <p id="description">"{{user.description}}"</p>
    {{#if isOwnProfile}}
    <a href="/profile/edit"><i class="far fa-edit"></i></a>
    {{/if}}
  <div class="slick-carousel">
    {{#each user.myEvents}}
    <div class="slide-content" id="wrapper"><a href="/events/{{this.id}}"><img id="joined-events" src="{{this.escapeRoom.image}}"></a></div>
    {{/each}}
  </div>
  </div>
</div>
